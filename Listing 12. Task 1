def setup():
    size(500,500)
    smooth()
    noLoop()
def draw():
    background(50)
    fill(94,206,40,250)
    strokeWeight(0)
    ellipse(250,100,140,140)
    fill(94,206,40,150)
    strokeWeight(0)
    ellipse(250,200,140,140)
    fill(94,206,40,100)
    strokeWeight(0)
    ellipse(250,300,140,140)
    fill(94,206,40,50)
    strokeWeight(0)
    ellipse(250,400,140,140)
