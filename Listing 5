displayWidth = 500
displayHeight = 500
ellipseSize = 100
ellipseWidth = 200
ellipseHeight = 300

def setup():
    size(displayWidth,displayHeight)
    smooth()
    background(255)
    fill(50,80)
    stroke(100)
    strokeWeight(3)
    noLoop()
    
def draw():
    ellipse(displayWidth/2,displayHeight/2-ellipseSize/2,ellipseWidth, ellipseHeight)
    ellipse(displayWidth/2-ellipseSize/2,displayHeight/2,ellipseWidth, ellipseHeight)
    ellipse(displayWidth/2+ellipseSize/2,displayHeight/2,ellipseWidth, ellipseHeight)
    ellipse(displayWidth/2,displayHeight/2+ellipseSize/2,ellipseWidth, ellipseHeight)
